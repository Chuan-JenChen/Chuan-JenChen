num1=int(input('數字'))
op=input('符號')
num2=int(input('數字'))
sum=[]

if op=='+':
    sum=num1+num2
    print(sum)
elif op=='-':
    sum=num1+num2
    print(sum) 
elif op=='*':
    sum=num1+num2
    print(sum)
elif op=='/':
    sum=num1+num2 
    print(sum)
else:
    print('輸入錯誤') 
    
    
